{
  "name": "@dxy-toh/biomart_pc",
  "version": "1.0.0",
  "description": "基于 React 的业务组件库",
  "private": false,
  "main": "dist/biomart_pc.umd.min.js",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "http://npm.dxy.net:7001"
  },
  "scripts": {},
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "output": {
    "path": "config.build.assetsRoot",
    "filename": "utils.assetsPath('js/[name].js')",
    "chunkFilename": "utils.assetsPath('js/[id].js')",
    "library": "TipUi",
    "libraryTarget": "umd"
  },
  "dependencies": {
    "docz": "^2.3.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@umijs/fabric": "^2.2.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "antd": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "components/**/*.{js,ts}?(x)": [
      "prettier --write",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
